{
  "name": "graduation",
  "version": "1.0.0",
  "description": "解决工作区eslint和prettier的问题",
  "main": ".prettierrc.js",
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "babel-eslint": "^10.0.3",
    "eslint-plugin-nestjs": "^1.2.3",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-plugin-vue": "^6.1.2",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "format": "prettier --write '*/.{js,jsx,css,less}'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "cad",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add ."
    ]
  }
}
